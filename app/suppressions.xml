<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
     "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
     "https://checkstyle.org/dtds/suppressions_1_0.dtd">

<suppressions>
	<!-- Ignore files pulled down from translations services -->
	<suppress files=".*\.i18n_.*\.properties" checks=".*" />
	<suppress files="LocalDescriptions_.*\.properties" checks=".*" />
	<suppress files="[\\/]test[\\/]" checks="[a-zA-Z0-9]*" />
	<suppress files="[\\/]resources[\\/]" checks="[a-zA-Z0-9]*" />
	<suppress files="[\\/]generated-sources[\\/]"
		checks="[a-zA-Z0-9]*" />


</suppressions>